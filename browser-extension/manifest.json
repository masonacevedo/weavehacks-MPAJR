{
    "name": "Weavehacks2025",
    "manifest_version": 3,
    "version": "1.0",
    "action": {
        "default_title": "Weavehacks2025"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "side_panel": {
        "default_path": "sidepanel.html"
    },
    "permissions": ["sidePanel", "activeTab"]
}